    "head2": {
        "type": "section",
        "description": "Popup elements appearance"
    },
    "pref_style_for_language_pair": {
        "type": "entry",
        "expand-width": true,
        "default": "font-weight: bold; font-size: 1.2em; padding-bottom: 5px;",
        "description": "Language pair style"
    },
    "pref_style_for_translated_text": {
        "type": "entry",
        "expand-width": true,
        "default": "font-size: 1.2em; padding-bottom: 5px;",
        "description": "Translated text style"
    },
    "pref_style_for_footer": {
        "type": "entry",
        "expand-width": true,
        "default": "font-weight: bold; font-size: 1.2em;",
        "description": "Footer style"
    },
    "head3": {
        "type": "section",
        "description": "Translation history settings"
    },
    "pref_history_timestamp": {
        "type": "combobox",
        "default": 0,
        "description": "Timestamp for translation history entries",
        "options": {
            "Custom": 0,
            "YYYY MM-DD hh:mm:ss (ISO8601)": 1,
            "YYYY DD-MM hh:mm:ss (European)": 2
        }
    },
    "pref_history_timestamp_custom": {
        "type": "entry",
        "expand-width": true,
        "default": "YYYY-MM-DD hh:mm:ss",
        "description": "Custom timestamp",
        "tooltip": "Choose a custom timestamp for the translation history entries.\nYYYY: year\nMM: month\nDD: day\nhh: hours\nmm: minutes\nss: seconds"
    },
    "pref_history_initial_window_width": {
        "type": "spinbutton",
        "default": 800,
        "min": 400,
        "max": 2048,
        "step": 50,
        "units": "pixels",
        "description": "History window initial width"
    },
    "pref_history_initial_window_height": {
        "type": "spinbutton",
        "default": 600,
        "min": 400,
        "max": 2048,
        "step": 50,
        "units": "pixels",
        "description": "History window initial height"
    },
    "pref_history_width_to_trigger_word_wrap": {
        "type": "spinbutton",
        "default": 300,
        "min": 100,
        "max": 1024,
        "step": 10,
        "units": "pixels",
        "description": "Width to trigger word wrap",
        "tooltip": "The \"Source text\" and \"Target text\" columns on the history window will wrap its text at the width defined by this setting."
    },
    "head4": {
        "type": "section",
        "description": "Yandex translator settings"
    },
    "pref_yandex_api_keys": {
        "type": "textview",
        "description": "API keys",
        "default": "",
        "tooltip": "Enter one API key per line.\nRead the HELP.md file (Help item on this applet context menu) found inside this applet folder to know how to get free Yandex API keys.",
        "height": 150
    },
    "pref_all_dependencies_met": {
        "type": "generic",
        "default": false
    }
}
