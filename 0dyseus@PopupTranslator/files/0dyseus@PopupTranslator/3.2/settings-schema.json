{
    "layout": {
        "type": "layout",
        "pages": [
            "page_mechanisms",
            "page_general"
        ],

        "page_mechanisms": {
            "type": "page",
            "title": "Translation Mechanisms",
            "sections": [
                "section_first_mechanism",
                "section_second_mechanism",
                "section_third_mechanism",
                "section_fourth_mechanism"
            ]
        },
        "page_general": {
            "type": "page",
            "title": "General",
            "sections": [
                "section_applet_settings",
                "section_appearance",
                "section_translation_history",
                "section_yandex_settings",
                "section_debugging_settings"
            ]
        },

        "section_applet_settings": {
            "type": "section",
            "title": "Applet settings",
            "keys": [
                "pref_custom_icon_for_applet",
                "pref_custom_label_for_applet"
            ]
        },
        "section_appearance": {
            "type": "section",
            "title": "Popup elements appearance",
            "keys": [
                "pref_style_for_language_pair",
                "pref_style_for_translated_text",
                "pref_style_for_footer"
            ]
        },
        "section_translation_history": {
            "type": "section",
            "title": "Translation history settings",
            "keys": [
                "pref_history_timestamp",
                "pref_history_timestamp_custom",
                "pref_history_initial_window_width",
                "pref_history_initial_window_height",
                "pref_history_width_to_trigger_word_wrap"
            ]
        },
        "section_yandex_settings": {
            "type": "section",
            "title": "Yandex translator settings",
            "keys": [
                "pref_yandex_api_keys"
            ]
        },
        "section_debugging_settings": {
            "type": "section",
            "title": "Debugging settings",
            "keys": [
                "debugging_label",
                "pref_loggin_enabled",
                "pref_loggin_save_history_indented"
            ]
        },
        "section_first_mechanism": {
            "type": "section",
            "title": "First translation mechanism (Left click on applet)",
            "keys": [
                "pref_service_provider_1",
                "pref_source_lang_1",
                "pref_target_lang_1",
                "pref_translate_key_1",
                "pref_translate_key_forced_1"
            ]
        },
        "section_second_mechanism": {
            "type": "section",
            "title": "Second translation mechanism (Middle click on applet)",
            "keys": [
                "pref_service_provider_2",
                "pref_source_lang_2",
                "pref_target_lang_2",
                "pref_translate_key_2",
                "pref_translate_key_forced_2"
            ]
        },
        "section_third_mechanism": {
            "type": "section",
            "title": "Third translation mechanism (Hotkey #1)",
            "keys": [
                "pref_service_provider_3",
                "pref_source_lang_3",
                "pref_target_lang_3",
                "pref_translate_key_3",
                "pref_translate_key_forced_3"
            ]
        },
        "section_fourth_mechanism": {
            "type": "section",
            "title": "Fourth translation mechanism (Hotkey #2)",
            "keys": [
                "pref_service_provider_4",
                "pref_source_lang_4",
                "pref_target_lang_4",
                "pref_translate_key_4",
                "pref_translate_key_forced_4"
            ]
        }
    },

