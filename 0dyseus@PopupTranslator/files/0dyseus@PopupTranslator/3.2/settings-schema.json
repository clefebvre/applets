    "head0": {
        "type": "section",
        "description": "Applet settings"
    },
    "pref_custom_icon_for_applet": {
        "type": "iconfilechooser",
        "expand-width": true,
        "default": "popup-translator-translate-symbolic",
        "description": "Custom icon"
    },
    "pref_custom_label_for_applet": {
        "type": "entry",
        "expand-width": true,
        "default": "",
        "description": "Custom label"
    },
    "pref_translate_key": {
        "type": "keybinding",
        "description": "Keyboard shortcut to perform a translation",
        "default": "",
        "tooltip": "Choose a keybinding to perform a translation."
    },
    "pref_force_translate_key": {
        "type": "keybinding",
        "description": "Keyboard shortcut to perform a FORCED translation",
        "default": "",
        "tooltip": "Choose a keybinding to perform a FORCED translation. A \"forced translation\" forces the translation of any string stored on the translation history and overwrites the existing one."
    },
