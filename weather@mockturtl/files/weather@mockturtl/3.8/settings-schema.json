{
	"layout": {
        "type":"layout",
        "pages":[
            "providers",
            "presentation-section",
			"units-section",
			"info-section"
        ],
        "providers":{
            "type":"page",
            "title":"Weather",
            "sections":[
				"provider-options",
				"weather-conditions"
            ]
        },
        "presentation-section":{
            "type":"page",
            "title":"Presentation",
            "sections":[
                "keybindings",
				"panel-options",
				"presentation"
            ]
        },
        "units-section":{
            "type":"page",
            "title":"Units and Icons",
            "sections":[
				"units",
				"icons"
            ]
		},
		"info-section":{
            "type":"page",
            "title":"Help",
            "sections":[
				"provider-info",
				"location-info"
			]
		},
		"provider-info" : {
			"type":"section",
			"title":"Weather provider details",
			"keys":[
				"openWM",
				"darkS",
				"METN",
				"WB",
				"Yahoo",
				"Climacell",
				"metuk",
				"usweather"
			]
		},
		"location-info": {
			"type":"section",
			"title":"Location information",
			"keys":[
				"loc-info"
			]
		},
		"provider-options" : {
			"type":"section",
			"title":"Weather Provider Options",
			"keys":[
				"dataService",
				"apiKey",
				"manualLocation",
				"location",
				"getLocation",
				"refreshInterval",
				"forecastDays",
				"forecastHours"
			]
		},
		"units" : {
			"type":"section",
			"title":"Units",
			"keys":[
				"temperatureUnit",
				"windSpeedUnit",
				"pressureUnit",
				"distanceUnit"
			]
		},
		"keybindings" : {
			"type":"section",
			"title":"Keybindings",
			"keys":[
				"keybinding"
			]
		},
		"panel-options" : {
			"type":"section",
			"title":"Panel (Taskbar)",
			"keys":[
				"showTextInPanel",
				"showCommentInPanel",
				"tempTextOverride"
			]
		},
		"presentation" : {
			"type":"section",
			"title":"Popup menu",
			"keys":[
				"verticalOrientation",
				"forecastColumns",
				"forecastRows",
				"showSunrise",
				"show24Hours",
				"temperatureHighFirst",
				"tempRussianStyle",
				"locationLabelOverride"
			]
		},
		"weather-conditions" : {
			"type":"section",
			"title":"Weather conditions",
			"keys":[
				"translateCondition",
				"shortConditions"
			]
		},
		"icons" : {
			"type":"section",
			"title":"Icons",
			"keys":[
				"useSymbolicIcons",
				"useCustomMenuIcons",
				"useCustomAppletIcons"
			]
		}
	},
	"keybinding": {
		"type": "keybinding",
		"description": "Set the keybinding to call the menu",
		"default": "<Super><Shift>w"
	},
	"dataService": {
		"type": "radiogroup",
		"default": "OpenWeatherMap",
		"description": "Data service",
		"options": {
			"OpenWeatherMap": "OpenWeatherMap",
			"DarkSky (key needed)": "DarkSky",
			"MET Norway": "MetNorway",
			"Weatherbit (key needed)": "Weatherbit",
			"Yahoo": "Yahoo",
			"Climacell (key needed)": "Climacell",
			"Met Office UK": "Met Office UK",
			"US National Weather": "US Weather"
		}
	},
	"apiKey": {
		"type": "entry",
		"default": "",
		"description": "API Key",
		"tooltip": "Copy this from your account of the used Data service and paste it here."
	},
	"manualLocation": {
		"type": "checkbox",
		"default": false,
		"description": "Manual Location",
		"tooltip": "Enable this if your location is not accurate"
	},
	"location": {
		"type": "entry",
		"default": "London,UK",
		"dependency": "manualLocation",
		"description": "Location",
		"tooltip": "Acceptable inputs are Latitude,Longtitude. e.g.: 68.1932,17.1426 or an address.\nWaits for 3 seconds after you stop typing then refreshes the weather."
	},
	"getLocation": {
		"type": "button",
		"indent": true,
		"description": "Setup API Key and Location",
		"callback": "locationLookup",
		"tooltip": "Opens webpage guide"
	},
	"refreshInterval": {
		"type": "spinbutton",
		"default": 15,
		"min": 5,
		"max": 360,
		"units": "minutes",
		"step": 5,
		"description": "Update interval"
	},
	"forecastDays": {
		"type": "spinbutton",
		"default": 3,
		"min": 2,
		"max": 16,
		"units": "days",
		"step": 1,
		"description": "Forecast length"
	},
	"forecastHours": {
		"type": "spinbutton",
		"default": 12,
		"min": 3,
		"max": 48,
		"units": "hours",
		"step": 1,
		"description": "Hourly Forecast length"
	},
	"temperatureUnit": {
		"type": "radiogroup",
		"default": "celsius",
		"description": "Temperature unit",
		"options": {
		"celsius": "celsius",
		"fahrenheit": "fahrenheit"
		}
	},
	"windSpeedUnit": {
		"type": "combobox",
		"default": "kph",
		"description": "Wind speed unit",
		"options": {
			"kph": "kph",
			"mph": "mph",
			"m/s": "m/s",
			"knots": "Knots",
			"Beaufort": "Beaufort"
		}
	},
	"pressureUnit": {
		"type": "combobox",
		"default": "hPa",
		"description": "Atmospheric pressure unit",
		"options": {
			"hPa": "hPa",
			"in Hg": "in Hg",
			"mm Hg": "mm Hg",
			"psi": "psi",
			"at": "at",
			"atm": "atm",
			"Pa": "Pa"
		}
	},
	"distanceUnit": {
		"type": "combobox",
		"default": "metric",
		"description": "Distance unit",
		"options": {
			"Metric": "metric",
			"Imperial": "imperial"
		}
	},
	"showTextInPanel": {
		"type": "checkbox",
		"default": true,
		"description": "Display current temperature in panel"
	},
	"showCommentInPanel": {
		"type": "checkbox",
		"default": true,
		"description": "Show the weather condition (e.g., \"Windy\", \"Clear\") in the panel",
		"tooltip": "Only works with horizontal panels"
	},
	"tempTextOverride": {
		"type": "entry",
		"default": "",
		"description": "Override label on panel",
		"tooltip": "Available values are: {c} is condition, {t} is temperature and {u} is unit.\nCan be used if label does not fit on vertical panel, among other smart things."
	},
	"useCustomAppletIcons": {
		"type": "checkbox",
		"default": false,
		"tootlip": "Iconset is weather-icons by Erik Flowers",
		"description": "Use custom icons on the panel"
	},
	"verticalOrientation": {
		"type": "checkbox",
		"default": false,
		"tooltop": "Displaying forecasts from top to bottom instead from left to right.",
		"description": "Show forecast vertically"
	},
	"forecastColumns": {
		"type": "spinbutton",
		"default": 5,
		"min": 1,
		"max": 16,
		"units": "columns",
		"step": 1,
		"description": "Maximum number of columns for forecast"
	},
	"forecastRows": {
		"type": "spinbutton",
		"default": 4,
		"min": 1,
		"max": 16,
		"units": "rows",
		"step": 1,
		"description": "Maximum number of rows for forecast"
	},
	"showSunrise": {
		"type": "checkbox",
		"default": true,
		"description": "Show sunrise / sunset times"
	},
	"show24Hours": {
		"type": "checkbox",
		"default": false,
		"description": "Display time in 24 hour format"
	},
	"temperatureHighFirst": {
		"type": "checkbox",
		"default": false,
		"description": "Show high temperature first in forecast"
	},
	"tempRussianStyle": {
		"type": "checkbox",
		"default": false,
		"description": "Show temperature Russian style"
	},
	"locationLabelOverride": {
		"type": "entry",
		"default": "",
		"description": "Override location label"
	},
	"translateCondition": {
		"type": "checkbox",
		"default": true,
		"description": "Translate conditions"
	},
	"shortConditions": {
		"type": "checkbox",
		"default": false,
		"description": "Less verbose conditions",
		"tooltip": "Currently only used with DarkSky "
	},
	"useSymbolicIcons": {
		"type": "checkbox",
		"default": false,
		"tooltip": "Does not apply to custom iconset, it is symbolic by default",
		"description": "Use symbolic icons throughout the applet"
	},
	"useCustomMenuIcons": {
		"type": "checkbox",
		"tootlip": "Iconset is weather-icons by Erik Flowers",
		"default": false,
		"description": "Use custom icons in the popup menu"
		
	},
	"openWM": {
		"type": "label",
		"description": "OpenWeatherMap:\nThe default, works out of the box.\nBig Thanks to them supporting free open source projects, like this!"
	},
	"darkS": {
		"type": "label",
		"description": "DarkSky:\nNeeds key, please click on 'Setup API Key and Location' for the steps how to obtain one."
	},
	"METN": {
		"type": "label",
		"description": "MET Norway:\nCurrent weather is shown for the next hour, and the daily forecasts are generated from 6 hour forecasts, so it can look incorrect sometimes."
	},
	"WB": {
		"type": "label",
		"description": "WeatherBit:\nAt least 10 minutes as refresh rate is recommended, since otherwise you might exceed you daily quota."
	},
	"Yahoo": {
		"type": "label",
		"description": "Yahoo:\nRequires 'python3-requests-oauthlib' package installed (for Arch based distros 'python-requests-oauthlib'), Updates every 2 hours."
	},
	"Climacell": {
		"type": "label",
		"description": "Empty"
	},
	"metuk": {
		"type": "label",
		"description": "Empty"
	},
	"usweather": {
		"type": "label",
		"description": "Empty"
	},
	"loc-info": {
		"type": "label",
		"description": "You can either use coordinates or an address, from smallest detail to biggest, like '{house num} {street} {city/town} {postcode} {country}'. All of them are optional, although at least city and country is recommended. You can use commas if you like."
	}
}