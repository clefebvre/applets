msgstr "Provjera"

#: settings-schema.json:37
msgid "Monitoring"
msgstr "Nadgledanje"

#: settings-schema.json:72
msgid "Frequency"
msgstr "Vrijeme provjere"

#: settings-schema.json:77
msgid "Notifications"
msgstr "Obavijesti"

