#. Cinnamenu@json->settings-schema.json->enable-windows->description
msgid "Enable searching of open windows"
msgstr "Permitir pesquisa de janelas abertas"

#. Cinnamenu@json->settings-schema.json->enable-windows->tooltip
msgid "Choose whether or not open windows are indexed in menu searches."
msgstr "Escolher se janelas abertas são indexadas ou não nas pesquisas"

#. Cinnamenu@json->settings-schema.json->enable-animation->description
msgid "Use menu animations"
msgstr "Usar animações do menu"

#. Cinnamenu@json->settings-schema.json->enable-animation->tooltip
msgid "Allow the menu to animate on open and close"
msgstr "Permite animações do menu ao abrir e fechar"

#. Cinnamenu@json->settings-schema.json->show-application-icons->description
msgid "Show application icons"
msgstr "Mostrar ícones de aplicações"

#. Cinnamenu@json->settings-schema.json->show-application-icons->tooltip
msgid "Choose whether or not to show icons on applications."
msgstr "Escolher mostrar ícones das aplicações ou não."

#. Cinnamenu@json->settings-schema.json->overlay-key->description
msgid "Keyboard shortcut to open and close the menu"
msgstr "Atalho de teclado para abrir e fechar o menu"

#. Cinnamenu@json->settings-schema.json->apps-grid-column-count->description
#. Cinnamenu@json->settings-schema.json->apps-grid-column-count->tooltip
msgid "Number of columns in apps grid"
msgstr "Número de colunas na vista de ícones"

#. Cinnamenu@json->settings-schema.json->apps-grid-column-count->units
msgid "columns"
msgstr "colunas"

#. Cinnamenu@json->settings-schema.json->show-tooltips->description
msgid "Show tooltips"
msgstr "Mostrar tooltips"

#. Cinnamenu@json->settings-schema.json->show-tooltips->tooltip
msgid ""
"When enabled, tooltips containing the item's name or description is shown "
"when hovered over."
msgstr "Quando ativo, tooltips com o nome ou descrição do ítem são mostradas "
"quando o rato se move sobre ele."

#. Cinnamenu@json->settings-schema.json->tooltip-delay->description
msgid "Tooltip delay"
msgstr "Atraso da tooltip"

#. Cinnamenu@json->settings-schema.json->tooltip-delay->tooltip
msgid ""
"The delay time in milliseconds for tooltips when hovering over a menu item."
msgstr "O atraso em milisegundos para tooltips quando o rato se move sobre um ítem."

#. Cinnamenu@json->settings-schema.json->tooltip-delay->units
#. Cinnamenu@json->settings-schema.json->hover-delay->units
msgid "milliseconds"
msgstr "milisegundos"

