#: applet.js:13
msgid "Press to enable Tor"
msgstr "Нажмите, чтобы включить Tor"

#: applet.js:14
msgid "Press to disable Tor"
msgstr "Нажмите, чтобы выключить Tor"

#: applet.js:80
msgid "Tor closed."
msgstr "Tor остановлен."

#: applet.js:86
msgid "Tor launched."
msgstr "Tor запущен."

#. tor-button@shatur->settings-schema.json->location->description
msgid "Tor location path"
msgstr "Расположение Tor"

#. tor-button@shatur->settings-schema.json->location->tooltip
msgid "Enter the command to launch Tor."
msgstr "Введите команду для запуска Tor."

#. tor-button@shatur->settings-schema.json->refreshInterval->description
msgid "Refresh Interval for check Tor activity:"
msgstr "Интервал обновления проверки статуса Tor:"

#. tor-button@shatur->settings-schema.json->refreshInterval->tooltip
msgid "Increase or decrease this spinner value to change the refresh interval for check Tor activity - use a slow refresh if you have a slow machine."
msgstr "Увеличить или уменьшить интервал обновления статуса Tor - используйте, если у вас слабая машина."

#. tor-button@shatur->settings-schema.json->showNotifications->description
msgid "Show notifications"
msgstr "Показывать уведомления"

#. tor-button@shatur->settings-schema.json->showNotifications->tooltip
msgid "Show Tor notifications launch."
msgstr "Показывать уведомления запуска Tor."

#. tor-button@shatur->metadata.json->name
msgid "Tor Button"
msgstr ""

#. tor-button@shatur->metadata.json->description
msgid "Click on the applet to enable or disable Tor"
msgstr "Нажмите на апплете, чтобы включить или выключить Tor"
